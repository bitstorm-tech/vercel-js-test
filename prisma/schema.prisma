generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model Account {
  id        Int        @id @default(autoincrement())
  email     String     @unique
  password  String
  dealer    Boolean    @default(false)
}
